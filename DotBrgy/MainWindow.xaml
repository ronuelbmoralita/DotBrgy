                                <materialDesign:PackIcon Kind="Spy" Width="20" Height="20" Margin="5" VerticalAlignment="Center"/>
                                <TextBlock Text="Admin" Margin="10"/>
                            </StackPanel>
                        </ListViewItem>


                        <ListViewItem Name="openTech" 
                                      Height="auto" 
                                      Padding="10" 
                                      MouseLeftButtonUp="OpenTech_MouseLeftButtonUp"
                                      ToolTip="Contact Developer">
