                                    <TextBlock Visibility="Collapsed" HorizontalAlignment="Center" VerticalAlignment="Center" Text="KonTra" FontSize="50"/>
                                    <materialDesign:PackIcon VerticalAlignment="Center" Margin="20" HorizontalAlignment="Center" Height="30" Foreground="#65ffa0" Width="30" Kind="Virus" />
                                </StackPanel>

                                <!--BUG-->
                                <Grid Margin="20" Height="100">
                                    <materialDesign:Card Width="auto" UniformCornerRadius="10" Height="100"/>
                                    <!--
                             <materialDesign:Card Width="auto"/>
                        <Grid Width="35" Height="30" Background="#FFFFAF24" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="10 0">
                            <materialDesign:PackIcon Kind="Magnify" HorizontalAlignment="Center" VerticalAlignment="Bottom" Margin="5" Foreground="White" Width="30" Height="30"/>
                        </Grid
                        <Grid Width="auto" Height="auto" Background="#FF3580BF" HorizontalAlignment="Left" VerticalAlignment="Top">
                            <materialDesign:PackIcon Kind="DatabaseSearch" Margin="5" Foreground="White" Width="30" Height="30"/>
                        </Grid -->

                                    <StackPanel Orientation="Horizontal"  VerticalAlignment="Center" Height="auto" Width="auto" Margin="20,0,20,0">
                                        <TextBox Name="SearchHistory"
                                                  materialDesign:TextFieldAssist.HasClearButton="True"
                                                  Width="250"  
                                                  BorderThickness="1" 
                                                  Style="{StaticResource MaterialDesignOutlinedTextBox}" 
                                                  FontSize="20" 
                                                  Height="auto"
                                                  TextChanged="SearchHistory_TextChanged"
                                                  ToolTip="Search Tracer">
                                            <materialDesign:HintAssist.Hint>
                                                <StackPanel Orientation="Horizontal" VerticalAlignment="Center" HorizontalAlignment="Center">
                                                    <materialDesign:PackIcon Kind="Search" Width="30" Height="30" VerticalAlignment="Center"/>
                                                    <TextBlock Text="Search" FontSize="20"/>
                                                </StackPanel>
                                            </materialDesign:HintAssist.Hint>
                                        </TextBox>
                                    </StackPanel>

                                    <StackPanel Margin="0,0,20,0" Width="auto" VerticalAlignment="Center" HorizontalAlignment="Right" Orientation="Horizontal">
                                        <StackPanel x:Name="numericContainerHistory" Visibility="Visible"  Orientation="Horizontal">
                                            <RadioButton x:Name="resCodeHistoryRadio" 
                                             Style="{StaticResource MaterialDesignChoiceChipPrimaryOutlineRadioButton}"
                                             Margin="5" 
                                             Content="Resident Code" Checked="resCodeHistoryRadio_Checked"/>
                                            <RadioButton x:Name="dateHistoryRadio"  
                                             Style="{StaticResource MaterialDesignChoiceChipPrimaryOutlineRadioButton}"     
                                             Margin="5" 
                                             Content="Date" Checked="dateHistoryRadio_Checked"/>
                                        </StackPanel>
                                        <Button Name="refreshTracker"
                                                Margin="5" 
                                                Content="Refresh"
                                                Style="{StaticResource MaterialDesignFlatLightBgButton}"
                                                materialDesign:ButtonAssist.CornerRadius="15"
                                                ToolTip="Refresh" Click="refreshTracker_Click"/>
                                    </StackPanel>
                                    <!--
                            <StackPanel Orientation="Horizontal" VerticalAlignment="Center" Margin="10"  HorizontalAlignment="Right">
                                <materialDesign:PackIcon Width="30" Height="30" Kind="Database" HorizontalAlignment="Center" Foreground="#b69b7d"/>
                                <Label  Margin="0,0,10,0" Content="count"  FontSize="20" Height="auto" Width="auto" Foreground="#b69b7d"/>
                            </StackPanel>
                            -->
                                </Grid>
                            </StackPanel>

                            <!--datagrid-->
                            <StackPanel DockPanel.Dock="Bottom" VerticalAlignment="Center" Height="auto" Width="auto">
                                <DataGrid Name="dbHistory" Margin="20,0,20,0" IsReadOnly="True" VerticalAlignment="Bottom"  CanUserSortColumns="True" Height="300" Background="AliceBlue">
                                    <!--
                            <DataGrid.ItemsPanel>
                                <ItemsPanelTemplate>
                                    <VirtualizingStackPanel VirtualizingStackPanel.IsVirtualizing="True" VirtualizingStackPanel.VirtualizationMode="Recycling"/>
                                </ItemsPanelTemplate>
                            </DataGrid.ItemsPanel>
                            -->
                                </DataGrid>
                            <StackPanel Orientation="Horizontal" Margin="20" VerticalAlignment="Center" HorizontalAlignment="Center">
                                    <Button Name="printHistory"
                                            Margin="5" 
                                            Content="Print"
                                            Style="{StaticResource MaterialDesignFlatLightBgButton}"
                                            materialDesign:ButtonAssist.CornerRadius="15"
                                            ToolTip="Print History" Click="printHistory_Click"/>

                                    <Button Name="DeleteAllHistory"   
                                            Margin="5" 
                                            Content="Delete"
                                            Style="{StaticResource MaterialDesignFlatAccentBgButton}"
                                            materialDesign:ButtonAssist.CornerRadius="15"
                                            ToolTip="Delete History"
                                            Click="DeleteAllHistory_Click"/>

                        </StackPanel>
                                <materialDesign:PopupBox Margin="20"
                                                         Style="{StaticResource MaterialDesignMultiFloatingActionPopupBox}"
                                                         StaysOpen="True"
                                                         PlacementMode="LeftAndAlignMiddles"
                                                         UnfurlOrientation="Horizontal"
                                                         ToolTip="Menu"
                                                         HorizontalAlignment="Right"
                                                         VerticalAlignment="Bottom"
                                                         Visibility="Collapsed">
                                    <StackPanel
                                      Orientation="Horizontal">
                                        <Button 
                                                Margin="5"
                                                Style="{StaticResource MaterialDesignFloatingActionButton}"
                                                Command="{Binding AddCommand}" 
                                                VerticalAlignment="Center"
                                                Padding="0"
                                                ToolTip="Print">
                                            <materialDesign:PackIcon Kind="Printer" Width="30" Height="30"/>
                                        </Button>
                                        <Button 
                                                Margin="5"
                                                VerticalAlignment="Center"
                                                Padding="0"
                                                ToolTip="Delete history" 
                                                Style="{StaticResource MaterialDesignFloatingActionAccentButton}"
                                                Command="{Binding RemoveSelectedItemCommand}">
                                            <materialDesign:PackIcon Kind="Delete" Width="30" Height="30"/>
                                        </Button>
                                    </StackPanel>
                                </materialDesign:PopupBox>
                            </StackPanel>
                        </StackPanel>
                    </DockPanel>
                </Grid>




                <Grid Width="650" HorizontalAlignment="Right">
                    <Border Background="#65ffa0" CornerRadius="20" BorderThickness="1" BorderBrush="White"/>
                   
                </Grid>
            </Grid>
        </Grid>
    </materialDesign:DialogHost>
</Window>
