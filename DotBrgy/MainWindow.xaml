                                        <materialDesign:PackIcon Kind="ArrowUpCircle" Width="auto" Height="auto" VerticalAlignment="Center"/>
                                    </StackPanel>
                                </materialDesign:HintAssist.Hint>
                            </TextBox>
                        </StackPanel>
                    </StackPanel>

                    <!--search-->
                    <StackPanel DockPanel.Dock="Top" Width="auto" Height="auto" Margin="0,20,0,0">
