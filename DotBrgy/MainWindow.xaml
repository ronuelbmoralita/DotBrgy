                                <materialDesign:PackIcon Kind="Phone" Width="20" Height="20" Margin="5" VerticalAlignment="Center"/>
                                <TextBlock Text="Technical Support" Margin="10"/>
                            </StackPanel>
                        </ListViewItem>


                        <ListViewItem Name="exit" 
                                      Height="auto" 
                                      Padding="10" 
                                      MouseLeftButtonUp="Exit_MouseLeftButtonUp"
                                      ToolTip="Close Window">
                            <StackPanel Orientation="Horizontal">
                                <materialDesign:PackIcon Kind="Close" Width="20" Height="20" Margin="5" VerticalAlignment="Center"/>
                                <TextBlock Text="Exit" Margin="10"/>
                            </StackPanel>
                        </ListViewItem>
                    </ListView>
                </StackPanel>
                <StackPanel VerticalAlignment="Top" HorizontalAlignment="Right" Margin="5">
                    <Button x:Name="closeLeft"
                            HorizontalAlignment="Right" 
                            VerticalAlignment="Top" 
                            Background="{x:Null}" 
                            BorderBrush="{x:Null}" 
                            Padding="0" 
                            ToolTip="Close"
                            Width="30"
                            Height="30"
                            Foreground="Black" Click="closeLeft_Click">
                        <materialDesign:PackIcon Kind="Close"/>
                    </Button>
                </StackPanel>
            </Grid>





            <!--history-->
            <Grid x:Name="History" Opacity="6.0" Width="1300" HorizontalAlignment="Right" VerticalAlignment="Top" Height="600"  Background="Transparent" RenderTransformOrigin="0.5,0.5" Grid.Column="1" Margin="0,0,-1300,0">
                <Grid.RenderTransform>
                    <TransformGroup>
                        <ScaleTransform/>
                        <SkewTransform/>
                        <RotateTransform/>
                        <TranslateTransform/>
                    </TransformGroup>
                </Grid.RenderTransform>



                <Border Background="Transparent" CornerRadius="20"/>
                <!--<materialDesign:Card  Width="auto" Height="700" Background="#fafafa" VerticalAlignment="Bottom"/>-->
                <!--<materialDesign:Card  Width="auto" Height="600" Background="#fafafa" VerticalAlignment="Bottom"/>-->           
                <!--<materialDesign:Card  UniformCornerRadius="0.1" Height="30" VerticalAlignment="Top" Background="#FF3580BF"/>-->

                
                <Grid Width="650" HorizontalAlignment="Left">
                    <Border BorderThickness="1" BorderBrush="#65ffa0" Background="#f6f6f6" CornerRadius="20" Padding="20"/>
                    <StackPanel VerticalAlignment="Top" HorizontalAlignment="Right" Margin="5">
                        <Button x:Name="closeRight" 
                            HorizontalAlignment="Right" 
                            Background="{x:Null}" BorderBrush="{x:Null}" 
                            Width="30" 
                            Height="30" 
                            Padding="0"
                            ToolTip="Close"
                            Foreground="Black" Click="closeRight_Click">
                            <materialDesign:PackIcon Kind="Close"/>
